{
  "name": "px-downloader",
  "version": "3.2.1",
  "description": "Download illust, manga, ugoira(animation) and novel from Pixiv",
  "main": "index.js",
  "scripts": {
    "postinstall": "cd ./vendor/jszip && npm i && cd ../gif.js && npm i",
    "watch": "gulp watch",
    "build": "gulp build",
    "clean": "gulp clean",
