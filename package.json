  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "del": "^3.0.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-zip": "^4.0.0",
    "vinyl-named": "^1.1.0",
    "webpack": "^3.5.5",
    "webpack-stream": "^4.0.0"
  }
